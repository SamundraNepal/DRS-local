generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  email      String @unique
  password   String
  auth_code  String
}

model Disease {
  id                 Int     @id @default(autoincrement())
  disease_image      String?
  disease_name       Json
  disease_conditions Json
  disease_treatments Json
}

model Doctors {
  id              Int    @id @default(autoincrement())
  doctor_expEn    String
  doctor_expNe    String
  doctor_imageURL String
  doctor_nameEn   String
  doctor_nameNe   String
  doctor_email    String
}
